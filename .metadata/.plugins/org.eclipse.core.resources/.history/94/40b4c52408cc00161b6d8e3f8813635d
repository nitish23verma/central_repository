	{
		String add="YES";
		String edit="YES";
		if(add.equals("YES")==true)
		{
			try {
				add();
			} catch (InterruptedException e) {
					e.printStackTrace();
			}
		} 
		 if(edit.equals("YES")==true)
		{
			try {
				edit();
			} catch (InterruptedException e) {
					e.printStackTrace();
			}
		}
	}
		
	

